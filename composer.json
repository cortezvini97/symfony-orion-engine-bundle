{
    "name": "cortezvini97/symfony-orion-engine-bundle",
    "version": "1.0.2",
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/cortezvini97",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Orion\\OrionEngine\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orion\\OrionEngine\\Tests\\":"tests/"
        }
    },
    "require": {
        "php":">=8.2",
        "symfony/console": "^7.0",
        "symfony/config": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "cortezvini97/orion": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1",
        "symfony/test-pack": "^1.0",
        "symfony/phpunit-bridge": "^7.0",
        "phpspec/prophecy-phpunit": "^2.2"
    }
}
